FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 2.53e-4;

boundaryField
{
    walls
    {
        type            kqRWallFunction;
        value           uniform 2.215e-07;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 2.215e-07;
    }
    vacuum
    {
        type            kqRWallFunction;
        value           uniform 2.215e-07;
    }
}


