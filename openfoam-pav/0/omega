FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 6.62E-5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 6.62E-5;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            omegaWallFunction;
        value           uniform 6.62E-5;
    }
    vacuum
    {
        type            omegaWallFunction;
        value           uniform 6.62E-5;
    }
}


// ************************************************************************* //